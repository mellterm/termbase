<%= form_for @profile do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :real_name %><br />
    <%= f.text_field :real_name %>
  </p>
  <p>
    <%= f.label :birthday %><br />
    <%= f.date_select :birthday %>
  </p>
  <p>
    <%= f.label :native_language_id %><br />
    <%= f.number_field :native_language_id %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.label :location_id %><br />
    <%= f.number_field :location_id %>
  </p>
  <p>
    <%= f.label :badge_id %><br />
    <%= f.number_field :badge_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
